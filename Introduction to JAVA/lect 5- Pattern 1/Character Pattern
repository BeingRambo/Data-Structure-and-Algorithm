import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {

Scanner s =new Scanner(System.in);
int n = s.nextInt();

int row =1;
while(row <=n)
{
char ch =(char)('A'+row-1);
int col =1;
while(col<=row)
{
System.out.print(ch);
ch=(char)(ch+1);
col=col+1;
}
System.out.println();
row= row +1;

}
		
	}

}
