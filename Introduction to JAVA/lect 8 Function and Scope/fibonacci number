import java.util.Scanner;
public class Solution {
	


	public static boolean checkMember(int n){
		int n1 =0;
	    int n2 =1;
	    if(n==0)
		{
         return true;
		}
	    boolean Fib = false;
		while(n1<n)
		{
		  int n3 = n1+n2;
		  n1 = n2;
		  n2  =n3;

		  if(n1==n)
		  {
			 Fib=true;

		  }
		

		}
		 return Fib;
	}

		public static void main (String args[])
		{
			Scanner s = new Scanner(System.in);
			int no = s.nextInt();

			boolean res =checkMember(no);
			System.out.println(res);


		

	}
	

}
