import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);

		int n = s.nextInt();
		
		int row = 1;
		while(row<=n)
		{
           int col =1;

		       while(col<=n-row+1)
		      {
         // focus on column decresing but how (n- row + 1)
	 
			      System.out.print(n-row+1);
			      col = col +1;
		       }
            System.out.println();
			row = row+1;

		} 
		
		
	}

}
