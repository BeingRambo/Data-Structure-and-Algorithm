
public class increment_and_decrement {
	
	public static void main (String[] args) {
	    int a=10,b=50;
	    // a becomes 11 using old value whenever we use post it wil use old value
	    a++;
	    // b becomes 49  pre decrement will use new value only for new pre- increment/dercrement
	    --b;
	    // but in this both are post decrement they uses old values(10 + 50)
	    int c=a--+b--;
	    // in this c is pre-incremented result (61)
	    System.out.print(++c);
	}
	}


